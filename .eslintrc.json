{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "import/no-extraneous-dependencies": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val" }],
    "spaced-comment": "off"
  }
}
